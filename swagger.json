{
    "openapi": "3.0.0",
    "info": {
        "title": "API Market Place",
        "description": "API de Market Place completa. Com o CRUD de vários assuntos",
        "contact": {
            "name": "Marcio",
            "email": "marcio@marcio.com"
        },
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://localhost3000/",
            "description": "API de teste local"
        },
        {
            "url": "https://aws.amazon.com.br/ijeieheu/market-place/",
            "description": "API de produção CLOUD"
        }
    ],
    "paths": {
        "/usuario/findAll": {
            "get": {
                "summary": "Lista todos os usuários da API",
                "description": "Rota responsável por todos os usuários da API",
                "tags": [
                    "Usuario"
                ],
                "responses":{
                    "200":{
                        "description": "Ok, tarefa realizada com sucesso"
                    },
                    "401":{
                        "description": "Unauthorized. Erro de autenticação, problemas com o token"
                    },
                    "404":{
                        "description": "Not found, nenhum usuário encontrado"
                    },
                    "500":{
                        "description": "Internal Server Erro, algo deu errado no servidor"
                    }
                }
            }
        },
        "/usuario/find": {
            "get": {
                "summary": "Lista o usuários que corresponde ao ID informado",
                "description": "Rota responsável por localizar um usuário da API através do ID informado",
                "tags": [
                    "Usuario"
                ]
            }
        }
    }
}